<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myteam">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Myteam">
        <activity
            android:name=".click_mylove_new"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="YOUR_GEO_API" />

        <activity
            android:name=".click_main_hotel" />
        <activity
            android:name=".click_attractions" />
        <activity
            android:name=".click_hotelchoose" />
        <activity
            android:name=".Hotelchoose_list"
            android:exported="false" />
        <activity
            android:name=".click_res" />
        <activity
            android:name=".attractions_list" />
        <activity
            android:name=".restaurant_list" />
        <activity
            android:name=".hotel_list" />
        <activity android:name=".input_schedule" />
        <activity
            android:name=".googleSignIn"
            android:exported="true" />
        <activity android:name=".GoogleMapsActivity" />
        <activity android:name=".forget_pwd" />
        <activity android:name=".register" />
        <activity android:name=".login" />
        <activity android:name=".hotel_main" />
        <activity android:name=".mySchedule" />
        <activity
            android:name=".MainActivity"
            android:logo="@drawable/logo"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>